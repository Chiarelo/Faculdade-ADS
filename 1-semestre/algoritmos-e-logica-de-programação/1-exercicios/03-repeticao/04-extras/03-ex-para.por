programa
{
	
	funcao inicio()
	{	
		inteiro i, result
		escreva("TABUADA DO 5")

		para(i =0; i<=10; i++){
			result = i * 5
			escreva(i, " * ", 5, " = ", result, "\n")
		}
	}
}
